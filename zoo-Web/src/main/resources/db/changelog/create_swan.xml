<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <!-- Refer to https://docs.liquibase.com/change-types/community/create-table.html -->

    <changeSet  failOnError="true" author="Juanperezaa" id="8caf1cbc-31e5-4fd6-aeec-64b1f0350f2d">

        <createTable tableName="SWAN">
            <column name="id" type="UUID">
                <constraints nullable="false" primaryKey="true" />
            </column>
            <column name="name" type="varchar(120)" />
            <column name="sex" type="varchar(255)" />
            <column name="weight" type="number" />
            <column name="age" type="number" />
            <column name="height" type="number" />
            <column name="arrival_date" type="varchar2(50)" />
            <column name="fatherid" type="UUID" />
            <column name="motherid" type="UUID" />
        </createTable>
    </changeSet>
</databaseChangeLog>